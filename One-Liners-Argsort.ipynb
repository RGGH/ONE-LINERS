    "## Argsort example"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 28,
   "metadata": {},
   "outputs": [],
   "source": [
    "import numpy as np\n",
    "\n",
    "sats_results = np.array([1100,1900,1400,1555,1000,1999])\n",
    "student_name = np.array([\"alice\",\"jim\",\"pete\",\"mofo\",\"dumb\",\"smartest\"])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 29,
   "metadata": {},
   "outputs": [],
   "source": [
    "top_3 = student_name[np.argsort([sats_results])][:-4:-1]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 30,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[['dumb' 'alice' 'pete' 'mofo' 'jim' 'smartest']]\n"
     ]
    }
   ],
   "source": [
    "print(top_3)"
   ]
  }
